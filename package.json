{
  "name": "undercast",
  "version": "1.1.0",
  "description": "A Mac & Windows Desktop App for Overcast with Media Key Support",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/*",
    "dist": "electron-builder --windows --mac",
    "start": "electron ."
  },
  "build": {
    "appId": "undercast",
    "productName": "Undercast",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imstuartjones/undercast"
  },
  "keywords": [
    "Overcast",
    "Undercast",
    "Electron",
    "NodeJS",
    "Node",
    "Menubar",
    "Applet"
  ],
  "author": "Stuart Jones",
  "license": "MIT",
  "dependencies": {
    "electron-default-menu": "^1.0.1",
    "menubar": "5.2.3"
  },
  "devDependencies": {
    "electron": "1.8.8",
    "electron-builder": "^22.4.1"
  }
}
